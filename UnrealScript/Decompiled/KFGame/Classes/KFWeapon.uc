        if(Instigator.Role < ROLE_Authority)
        {
            SendClientProjectileImpact(CurrentFireMode, Impact, PenetrationValue);
        }
        ProcessInstantHitEx(CurrentFireMode, Impact,, PenetrationValue);
    }
}

