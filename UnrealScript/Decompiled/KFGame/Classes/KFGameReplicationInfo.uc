                    bFoundZed = true;
                    bNetDirty = true;
                    goto J0x68C;
                }
                ++ I;
                goto J0x2AE;
            }
            J0x68C:

            if(!bFoundZed)
            {
                I = 0;
                J0x6A6:

                if(I < 32)
                {
                    if(ZedInfos[I].Zed == none)
                    {
                        ZedInfos[I].ZedLocation = KFPM.Location;
                        ZedInfos[I].Zed = KFPM;
                        ZedInfos[I].ZedClass = KFPM.Class;
