{
    super(KFGameInfo).TickZedTime(DeltaTime);
    if(ActiveEvent.bPermanentZedTime && IsWaveActive())
    {
        if((MyKFGRI.AIRemaining > ActiveEvent.PermanentZedTimeCutoff) || WaveNum == WaveMax)
        {
            ZedTimeRemaining = 999999;            
